# Expo Configuration
EXPO_PUBLIC_API_URL=https://api.yourdomain.com
EXPO_PUBLIC_WS_URL=wss://api.yourdomain.com

# Authentication
EXPO_PUBLIC_AUTH_URL=https://yourdomain.com/auth

# External Services
EXPO_PUBLIC_UPLOADCARE_PUBLIC_KEY=your_uploadcare_public_key
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Analytics
EXPO_PUBLIC_SENTRY_DSN=your_sentry_dsn
EXPO_PUBLIC_ANALYTICS_ENABLED=true

# Development/Production
NODE_ENV=production
EXPO_PUBLIC_ENVIRONMENT=production

# Feature Flags
EXPO_PUBLIC_ENABLE_CRASH_REPORTING=true
EXPO_PUBLIC_ENABLE_ANALYTICS=true
EXPO_PUBLIC_ENABLE_DEBUG_MODE=false

# API Keys (for mobile-specific services)
EXPO_PUBLIC_AMPLITUDE_API_KEY=your_amplitude_api_key
EXPO_PUBLIC_ONE_SIGNAL_APP_ID=your_one_signal_app_id

# Build Configuration
EXPO_PUBLIC_BUNDLE_ID=com.yourcompany.createanything
EXPO_PUBLIC_ANDROID_PACKAGE=com.yourcompany.createanything
